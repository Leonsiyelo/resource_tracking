- title("New Project")

= render :partial => 'projects/sub_nav'

= render 'breadcrumb', :response => @data_response, :project => @project

%h1.main_heading New Project

= render 'help'

= error_messages_for :project,  :header_message  => nil, :message => "Oops, we couldn't save your changes."

- semantic_form_for @project, :url => response_projects_path(@data_response), :html => {:class => "basic_form"} do |f|
  = render 'form', :f => f
