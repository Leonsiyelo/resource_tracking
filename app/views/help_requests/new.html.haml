- title "Contact / Help"

:javascript
  $('tabs').hide();

.sign_in_form
  - form_for @help_request, :url => help_requests_path do |form|
    %fieldset.user_form
      %p
        = form.label :email, "Email"
        = form.text_field :email
      %p
        = form.label :message, "Message"
      %p
        = form.text_area :message
      .buttons
        = form.submit "Submit"
