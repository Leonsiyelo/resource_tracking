%tr.js_project_row
  %td.name{:rowspan => project.in_flows.length + 2}= project.name
- project.in_flows.ordered_by_id.each do |implementer|
  = render 'implementer_row', :implementer => implementer, :type => params[:id]
%tr.filler2.js_project_total_row{:"data-project_id" => project.id}
  %td{:colspan => 5}
    - link_to new_response_implementer_path(@response), :class => 'add_funder add_row' do
      = image_tag "new.png"
      Add implementer
  %td.total Subtotal
  %td.total_amount
    %span.js_project_total= project.total_by_type(params[:id])
    %span.currency= project.currency
